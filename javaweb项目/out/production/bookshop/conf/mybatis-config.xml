        <typeAlias alias="ProductCommentRecord" type="com.bookshop.model.product.ProductCommentRecord"/>
    </typeAliases>
    <mappers>  
        <mapper resource="com.bookshop.model.conf.userManageMapper.xml" />
        <mapper resource="com.bookshop.model.conf.userFundsMapper.xml" />
        <mapper resource="com.bookshop.model.conf.AddressManageMapper.xml" />
